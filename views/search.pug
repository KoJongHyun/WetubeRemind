
extends layouts/main
include mixins/videoBlock

block content
  .container
    h3 Searching by #{searchingBy}
    .videos
      if videos.length === 0
        h5 Videos not found
      each item in videos
        +videoBlock({
          id: item.id,
          title: item.title,
          views: item.views,
          videoFile: item.videoFile
        })